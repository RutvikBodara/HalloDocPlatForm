                </button>
                </td> *@
                        <td class="text-white" style="background-color:@background">
                            <div class="btn-group">
                                <button type="button" class="btn btn-outline-light" data-bs-toggle="dropdown" data-bs-display="static" aria-expanded="false">
                                    Actions
                                </button>
                                <ul class="dropdown-menu dropdown-menu-lg-end">
                                    <li onclick="viewCases(@obj.RequestId)"><button class="dropdown-item" type="button">View Case</button></li>
                                    <li onclick="View_Document(@obj.RequestId)"><button class="dropdown-item" type="button">View Uploads</button></li>
                                    <li onclick="ShowViewNotes(@obj.RequestId)"><button class="dropdown-item" type="button">View Notes</button></li>
                                    <li onclick="OrderCheckOutPage(@obj.RequestId)"><button class="dropdown-item" type="button">Order</button></li>
                                    @* <li><button class="dropdown-item" type="button">Doctor Note</button></li> *@
                                    <li onclick="EncounterForm(@obj.RequestId)"><button class="dropdown-item" type="button">Encounter</button></li>
                                </ul>
                            </div>
                        </td>
                    </tr>
                }

            </tboday>
        </table>
        <div class="d-flex justify-content-between">
            <span class="justify-content-start me-3 "> @Model.PageNumber  of @Model.maxPage </span>
            <div class="d-flex justify-content-end mt-4 mb-5 gap-1">
                <button class="btn btn-primary " style="border-radius:10%" onclick="FirstPage()">
                    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="white" class="bi bi-chevron-double-left" viewBox="0 0 16 16">
                        <path fill-rule="evenodd" d="M8.354 1.646a.5.5 0 0 1 0 .708L2.707 8l5.647 5.646a.5.5 0 0 1-.708.708l-6-6a.5.5 0 0 1 0-.708l6-6a.5.5 0 0 1 .708 0" />
                        <path fill-rule="evenodd" d="M12.354 1.646a.5.5 0 0 1 0 .708L6.707 8l5.647 5.646a.5.5 0 0 1-.708.708l-6-6a.5.5 0 0 1 0-.708l6-6a.5.5 0 0 1 .708 0" />
                    </svg>
                </button>
                <button class="btn btn-primary" style="border-radius:10%" onclick="Previous()">Prev</button>
                <input id="PageNumber" type="number" class="pt-2 pb-2" value="@Model.PageNumber" style="border:none; border-radius:50%; width:50px; height:50px; font-family:500; text-align:center; " disabled />
                <button class="btn btn-primary" style="border-radius:10%" onclick="Next('@Model.maxPage')">Next</button>
                <button class="btn btn-primary me-5" style="border-radius:10%" onclick="lastPage()">
                    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-chevron-double-right" viewBox="0 0 16 16">
                        <path fill-rule="evenodd" d="M3.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L9.293 8 3.646 2.354a.5.5 0 0 1 0-.708" />
                        <path fill-rule="evenodd" d="M7.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L13.293 8 7.646 2.354a.5.5 0 0 1 0-.708" />
                    </svg>
                </button>
            </div>
        </div>
    </div>
}
else
{
    <div class="d-flex justify-content-center text-danger h3">
        Data Not Found
    </div>
}

<div class="d-block d-md-none patienttable">
    @if (Model.maxPage != 0)
    {
        @foreach (var obj in Model.ActiveRequestViewModels)
        {
            var uuid = Guid.NewGuid();
            <div class="shadow p-2" @* style="background-color:#e4f8ff" *@>
                <div class="pt-2 ps-3" data-bs-toggle="collapse" href="#A-@uuid" aria-expanded="false" aria-controls="#A-@uuid">

                    <div class="d-flex justify-content-between mb-3">
                        <div>
                            @obj.Firstname
                        </div>
                        <div class="me-3">
                            @if (obj.RequestType == 1)
                            {
                                <span>Business</span>
                                <svg height="15" width="15" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
                                    <circle fill="#e26277" r="45" cx="50" cy="50" stroke="grey" stroke-width="3" />
                                </svg>
                            }
                            else if (obj.RequestType == 2)
                            {
                                <span>Patient</span>
                                <svg height="15" width="15" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
                                    <circle fill="#198754" r="45" cx="50" cy="50" stroke="grey" stroke-width="3" />
                                </svg>
                            }
                            else if (obj.RequestType == 3)
                            {
                                <span>Family</span>
                                <svg height="15" width="15" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
                                    <circle fill="#ed9124" r="45" cx="50" cy="50" stroke="grey" stroke-width="3" />
                                </svg>
                            }
                            else if (obj.RequestType == 4)
                            {
                                <span>Concierge</span>
                                <svg height="15" width="15" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
                                    <circle fill="#00bafa" r="45" cx="50" cy="50" stroke="grey" stroke-width="3" />
                                </svg>
                            }
                        </div>
                    </div>

                    <div class="d-flex justify-content-between">
                        <p class="">
                            @obj.Address
                        </p>
                        <div class="me-3">
                            <a href="http://maps.google.com/?q={@obj.Address}" target="_blank" class="mapbtn btn btn-outline-primary"><span><i class="bi bi-geo-alt"></i></span></a>
                        </div>
                    </div>
                </div>
                <hr />
                <div class="collapse multi-collapse col-sm-12 ps-3 in" id="A-@uuid">
                    <div class="ms-2 mt-2">
                        <span>
                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-calendar3" viewBox="0 0 16 16">
                                <path d="M14 0H2a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2M1 3.857C1 3.384 1.448 3 2 3h12c.552 0 1 .384 1 .857v10.286c0 .473-.448.857-1 .857H2c-.552 0-1-.384-1-.857z" />
                                <path d="M6.5 7a1 1 0 1 0 0-2 1 1 0 0 0 0 2m3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2m3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2m-9 3a1 1 0 1 0 0-2 1 1 0 0 0 0 2m3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2m3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2m3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2m-9 3a1 1 0 1 0 0-2 1 1 0 0 0 0 2m3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2m3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2" />
                            </svg>
                        </span>
                        Date Of birth:
                        <span class="fw-bold">

                            @if (@obj.DateOfBirth != null)
                            {
                                @obj.DateOfBirth
                            }
                            else
                            {
                                <span class="fw-bold">Not Provided</span>
                            }
                        </span>
                    </div>

                    <div class="ms-2 mt-2 ">
                        <span>
                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#60cef3" class="bi bi-envelope" viewBox="0 0 16 16">
                                <path d="M0 4a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2zm2-1a1 1 0 0 0-1 1v.217l7 4.2 7-4.2V4a1 1 0 0 0-1-1zm13 2.383-4.708 2.825L15 11.105zm-.034 6.876-5.64-3.471L8 9.583l-1.326-.795-5.64 3.47A1 1 0 0 0 2 13h12a1 1 0 0 0 .966-.741M1 11.105l4.708-2.897L1 5.383z" />
                            </svg>
                        </span>
                        Email:
                        <span class="fw-bold">
                            @obj.Email
                        </span>
                    </div>

                    <div class="ms-2 mt-2">
