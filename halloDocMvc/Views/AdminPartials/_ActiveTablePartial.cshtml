                    <div class="ms-2 mt-2">
                        <span>
                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#60cef3" class="bi bi-person" viewBox="0 0 16 16">
                                <path d="M8 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6m2-3a2 2 0 1 1-4 0 2 2 0 0 1 4 0m4 8c0 1-1 1-1 1H3s-1 0-1-1 1-4 6-4 6 3 6 4m-1-.004c-.001-.246-.154-.986-.832-1.664C11.516 10.68 10.289 10 8 10s-3.516.68-4.168 1.332c-.678.678-.83 1.418-.832 1.664z" />
                            </svg>
                        </span>
                        Requestor
                        <span class="fw-bold">
                            :@obj.Requestor
                        </span>
                    </div>

                    <div class="d-flex row mt-3 gap-2 ms-2">
                        <button onclick="ShowViewNotes(@obj.RequestId)" class="btn col-5 text-white" style="background-color:#198754">View Notes</button>
                        <button onclick="viewCases(@obj.RequestId)" class="viewCases btn col-5 text-white" style="background-color:blue">View Case</button>
                        <button onclick="View_Document(@obj.RequestId)" id="CancelList" class="btn col-5 text-white" style="background-color:#198754"> View Uploads</button>
                        <button onclick="EncounterForm(@obj.RequestId)" id="CancelList" class="btn col-5 text-white" style="background-color:#198754"> Encounter</button>
                        <button onclick="OrderCheckOutPage(@obj.RequestId)" id="CancelList" class="btn col-5 text-white" style="background-color:orange"> Orders</button>
                        <button class="btn col-5 text-white" onclick="window.open('mailto:@obj.Email')" style="background-color:#198754">Email</button>
                    </div>
                </div>
            </div>
            <br />
        }

        <div class="d-flex justify-content-center mt-4 pb-5 gap-1">
            <button class="btn btn-primary " style="border-radius:10%" onclick="FirstPage()">
                <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="white" class="bi bi-chevron-double-left" viewBox="0 0 16 16">
