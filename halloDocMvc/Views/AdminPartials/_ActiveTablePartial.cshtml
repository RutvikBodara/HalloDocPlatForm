@if (Model.maxPage != 0)
{
    <div class="table-responsive d-none d-md-block mb-5">
        <table id="ActiveRequestsExcel" class="table" style="border:none">
            <thead>
                <tr>
                    <td>Name</td>
                    <td></td>
                    <td>DateOfBirth</td>
                    <td>Requestor</td>
                    <td>Physician Name</td>
                    <td>Date Of Service</td>
                    <td>phone</td>
                    <td>Address</td>
                    <td>Notes</td>
                    @* <td>Chat With</td> *@
                    <td>Action</td>
                </tr>
            </thead>
            <tboday>
                @foreach (var obj in Model.ActiveRequestViewModels)
                {

                    var background = "";

                    var requestor = "";
                    switch (obj.RequestType)
                    {
                        case 1:
                            background = "rgb(226, 98, 119)";
                            requestor = "business";
                            break;

                        case 2:
                            background = "rgb(25, 135, 84)";
                            break;

                        case 3:
                            background = "rgb(237, 145, 36)";
                            requestor = "Family";
                            break;
                        case 4:
                            background = "rgb(0, 186, 250)";
                            requestor = "Concierge";
                            break;
                    }



                    <tr class="bg-danger text-white">
                        <td class="text-white" style="background-color:@background">
                            @obj.Firstname
                        </td>
                        <td class="text-white" style="background-color:@background"><button onclick="window.open('mailto:@obj.Email')" class="btn btn-outline-light"><i class="bi bi-envelope"></i> </button></td>

                        <td class="text-white" style="background-color:@background">@obj.DateOfBirth</td>
                        <td class="text-white" style="background-color:@background">@obj.Requestor</td>
                        <td class="text-white" style="background-color:@background">@obj.PhysicianName</td>
                        <td class="text-white" style="background-color:@background">@obj.DateOfService</td>
                        <td class="text-white" style="background-color:@background">
                            <button class="btn btn-outline-light" onclick="window.open('tel:@obj.Phonenumber');">
                                <i class="bi bi-telephone"></i>
                                @obj.Phonenumber (Patient)
                            </button>
                            <br /><br />
                            @if (obj.RequestType != 2)
                            {
                                <button class="btn btn-outline-light" onclick="window.open('tel:@obj.MobileRequestor');">
                                    <i class="bi bi-telephone"></i>
                                    @obj.MobileRequestor (@requestor)
                                </button>
                            }
                        </td>
                        <td class="text-white" style="background-color:@background;">@obj.Address</td>
                        <td class="text-white" style="background-color:@background">@obj.Notes</td>
